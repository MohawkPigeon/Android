<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Memester">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Memester"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.Memester.MainActivity" android:screenOrientation="fullSensor"/>
        <activity android:name="com.example.Memester.Activitys.EditMemeView" android:screenOrientation="fullSensor"/>
        <activity android:name="com.example.Memester.Activitys.RecyclerViewView" android:screenOrientation="fullSensor"/>
        <activity android:name="com.example.Memester.Activitys.SavedMemesView" android:screenOrientation="fullSensor"/>
        <activity android:name=".FirebaseActivity.FirebaseActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>